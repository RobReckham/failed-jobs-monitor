{
    "name": "yay-innovations/failed-jobs-monitor",
    "type": "project",
    "description": "View + retry + delete failed jobs.",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "illuminate/config": "^5.8",
        "illuminate/contracts": "^5.8",
        "illuminate/routing": "^5.8",
        "illuminate/support": "^5.8"
    },
    "require-dev": {},
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "YayInnovations\\FailedJobsMonitor\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
}
